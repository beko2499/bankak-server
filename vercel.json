{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "admin/**/*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/admin/login.html",
            "dest": "/admin/login.html"
        },
        {
            "src": "/admin/register.html",
            "dest": "/admin/register.html"
        },
        {
            "src": "/register",
            "dest": "/admin/register.html"
        },
        {
            "src": "/admin/index.html",
            "dest": "/admin/index.html"
        },
        {
            "src": "/admin/?$",
            "dest": "/admin/index.html"
        },
        {
            "src": "^/$",
            "dest": "/admin/register.html"
        }
    ]
}